<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainMenu">

    <ImageButton
        android:id="@+id/infoButton"
        android:layout_width="190dp"
        android:layout_height="187dp"
        android:scaleType="fitCenter"
        android:src="@drawable/_botonnoticias"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/webButton"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/webButton"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/perfilButton"
        android:layout_width="190dp"
        android:layout_height="187dp"
        android:scaleType="fitCenter"
        android:src="@drawable/_perfil"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/donacionesButton"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/donacionesButton"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/eventosButton"
        android:layout_width="190dp"
        android:layout_height="187dp"
        android:scaleType="fitCenter"
        android:src="@drawable/_eventos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/logout"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/donacionesButton" />

    <ImageButton
        android:id="@+id/logout"
        android:layout_width="190dp"
        android:layout_height="187dp"
        android:scaleType="fitCenter"
        android:src="@drawable/_logoutbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/donacionesButton" />

    <ImageButton
        android:id="@+id/webButton"
        android:layout_width="190dp"
        android:layout_height="187dp"
        android:scaleType="fitCenter"
        android:src="@drawable/_visitarpagina"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04000002" />

    <ImageButton
        android:id="@+id/donacionesButton"
        android:layout_width="190dp"
        android:layout_height="187dp"
        android:scaleType="fitCenter"
        android:src="@drawable/_historial"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/webButton" />
</androidx.constraintlayout.widget.ConstraintLayout>
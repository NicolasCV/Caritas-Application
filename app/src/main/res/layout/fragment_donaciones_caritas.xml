<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DonacionesCaritas">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/nombre_donaciones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:text="nombre_perfil_aqui"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/datoTipoDonante"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/datoEstatusDonante"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/fechaTextDonaciones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="32dp"
        android:text="Fecha:"
        app:layout_constraintBottom_toTopOf="@+id/regresar"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/datoEstatusDonante"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/datoEstatusDonante2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/datoEstatusDonante2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/datoImporte"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/importeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="32dp"
        android:text="Importe:"
        app:layout_constraintBottom_toTopOf="@+id/fechaTextDonaciones"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/estatusText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="32dp"
        android:text="Estatus Donante:"
        app:layout_constraintBottom_toTopOf="@+id/ultimoDonativoText"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/datoFechaDonaciones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/regresar"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/ultimoDonativoText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="32dp"
        android:text="Ultimo Donativo"
        android:textAllCaps="true"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/importeText"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tipoDonanteText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="32dp"
        android:text="Tipo Donante:"
        app:layout_constraintBottom_toTopOf="@+id/estatusText"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/datoImporte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/datoFechaDonaciones"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/graphPrueba"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:src="@drawable/baseline_bar_chart_24"
        app:layout_constraintBottom_toTopOf="@+id/tipoDonanteText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nombre_donaciones" />

    <Button
        android:id="@+id/regresar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="64dp"
        android:text="Regresar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>